{
    "name": "dueclic/authpress",
    "description": "WordPress plugin for two-factor authentication with Telegram",
    "type": "wordpress-plugin",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "dueclic",
            "homepage": "https://www.dueclic.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "bacon/bacon-qr-code": "^2.0",
        "symfony/polyfill-iconv": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Authpress\\": "includes/",
            "AuthPress\\Providers\\": "includes/providers/"
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "optimize-autoloader": true
    }
}